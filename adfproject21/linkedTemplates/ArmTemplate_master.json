{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfproject21"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"SynapseLinkedServices_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SynapseLinkedServices'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen2project.dfs.core.windows.net/"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/ansil21/lung-cancer-dataset/main/lung_cancer_data.csv"
		},
		"SynapseLinkedServices_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "synapse-workspace21.sql.azuresynapse.net"
		},
		"SynapseLinkedServices_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datawarehouse21"
		},
		"SynapseLinkedServices_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ansil21"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfproject21_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"SynapseLinkedServices_password": {
						"value": "[parameters('SynapseLinkedServices_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"SynapseLinkedServices_properties_typeProperties_server": {
						"value": "[parameters('SynapseLinkedServices_properties_typeProperties_server')]"
					},
					"SynapseLinkedServices_properties_typeProperties_database": {
						"value": "[parameters('SynapseLinkedServices_properties_typeProperties_database')]"
					},
					"SynapseLinkedServices_properties_typeProperties_userName": {
						"value": "[parameters('SynapseLinkedServices_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}